<% provide(:title, "New Project") %>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>New Project</h1>
    <%= form_for(:project, url: projects_path) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.submit "Create Project", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
