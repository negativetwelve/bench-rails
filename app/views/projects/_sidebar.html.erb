<h4>Projects</h4>
<%= link_to "New Project", new_project_path, class: "btn btn-primary" %>

<div class="row">
  <div class="col-md-12">
    <% if signed_in? %>
      <ul>
        <% @projects.each do |project| %>
          <li><%= link_to project.name, project_select_path(project), remote: true %></li>
       <% end %>
      </ul>
    <% end %>
  </div>
</div>
